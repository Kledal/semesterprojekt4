        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav">
                <li class="active">
                    <a href="@Url.Action("Index", "Frontend")">
                        <i class="glyphicon glyphicon-home"></i> Oversigt <span class="sr-only">(current)</span>
                    </a>
                </li>
                <li ><a href="mine_bookinger.html"><i class="glyphicon glyphicon-th-list"></i> Mine bookinger</a></li>
            </ul>

            <ul class="nav navbar-nav navbar-right">
                <li><a href="#">Log ud</a></li>
            </ul>
        </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
</nav>

@RenderBody()

@Scripts.Render("~/bundles/jquery")
@RenderSection("scripts", required: false)
<!-- Latest compiled and minified JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>
</body>
</html>
